<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <Button
        android:id="@+id/InputList"
        android:layout_width="111dp"
        android:layout_height="34dp"
        android:layout_below="@+id/coupangpwText"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/edit_round"
        android:text="수동지출내역입력"
        android:textColor="#ffffff"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="44dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="월"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/frag2_month"
        app:layout_constraintEnd_toStartOf="@+id/frag2_day"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/frag2_month"
        app:layout_constraintTop_toTopOf="@+id/frag2_month"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/frag2_month"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginEnd="180dp"
        android:text="12"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/prevmonth2"
        app:layout_constraintEnd_toStartOf="@+id/nextMonth2"
        app:layout_constraintTop_toTopOf="@+id/prevmonth2" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="412dp"
        android:layout_height="436dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MonthPrice2"
        app:layout_constraintVertical_bias="0.506">

        <ListView
            android:id="@+id/listView2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:divider="@color/white"
            android:dividerHeight="3dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/dd2"
        android:layout_width="124dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="이 날 쓴 금액"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/MonthPrice2"
        app:layout_constraintEnd_toStartOf="@+id/textView112"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prevmonth2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/frag2_day"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="4dp"
        android:text="12"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView102"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/frag2_month"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/MonthPrice2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yearSave2" />

    <ImageButton
        android:id="@+id/prevmonth2"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/next2" />

    <ImageButton
        android:id="@+id/nextMonth2"
        android:layout_width="45dp"
        android:layout_height="49dp"
        android:layout_marginEnd="36dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/textView102"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView102"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/next3" />

    <TextView
        android:id="@+id/yearSave2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="120dp"
        android:text="."
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView102"
        android:layout_width="44dp"
        android:layout_height="43dp"
        android:layout_marginEnd="32dp"
        android:text="일"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/frag2_day"
        app:layout_constraintEnd_toStartOf="@+id/nextMonth2"
        app:layout_constraintTop_toTopOf="@+id/frag2_day"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView112"
        android:layout_width="50dp"
        android:layout_height="53dp"
        android:layout_marginEnd="8dp"
        android:text="원"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/MonthPrice2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/MonthPrice2"
        app:layout_constraintTop_toTopOf="@+id/MonthPrice2"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>